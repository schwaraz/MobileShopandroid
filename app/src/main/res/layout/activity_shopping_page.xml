<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ShoppingPage"
    tools:layout_editor_absoluteX="-7dp"
    tools:layout_editor_absoluteY="22dp">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="40dp"
        android:text="Nama toko:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/gula"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="30dp"
        android:text="Gula : @KG"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/beras" />

    <TextView
        android:id="@+id/garam"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="30dp"
        android:text="Garam : @KG"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/minyak" />

    <TextView
        android:id="@+id/telur"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="30dp"
        android:text="Telur : @KG"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/garam" />

    <TextView
        android:id="@+id/beras"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="12dp"
        android:text="Beras : @KG"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tdaftarbelanja" />

    <TextView
        android:id="@+id/tdaftarbelanja"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="48dp"
        android:text="list produk"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/minyak"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="30dp"
        android:text="Minyak 100L :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gula" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="48dp"
        android:text="Stock toko"
        app:layout_constraintStart_toEndOf="@+id/tdaftarbelanja"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/Sminyak"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:text="12"
        app:layout_constraintStart_toEndOf="@+id/minyak"
        app:layout_constraintTop_toBottomOf="@+id/Sgula" />

    <TextView
        android:id="@+id/Stelur"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="28dp"
        android:text="10"
        app:layout_constraintStart_toEndOf="@+id/telur"
        app:layout_constraintTop_toBottomOf="@+id/Sgaram" />

    <TextView
        android:id="@+id/Sgula"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="28dp"
        android:text="5"
        app:layout_constraintStart_toEndOf="@+id/gula"
        app:layout_constraintTop_toBottomOf="@+id/Sberas" />

    <TextView
        android:id="@+id/Sgaram"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:text="40"
        app:layout_constraintStart_toEndOf="@+id/garam"
        app:layout_constraintTop_toBottomOf="@+id/Sminyak" />

    <TextView
        android:id="@+id/Sberas"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:text="10"
        app:layout_constraintStart_toEndOf="@+id/beras"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginTop="48dp"
        android:text="Total beli"
        app:layout_constraintStart_toEndOf="@+id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/btelur"
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:layout_marginStart="150dp"
        android:ems="10"
        android:inputType="number"
        android:hint="0"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/Stelur"
        app:layout_constraintTop_toBottomOf="@+id/bgaram" />

    <EditText
        android:id="@+id/bberas"
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:layout_marginStart="150dp"
        android:ems="10"
        android:hint="0"
        android:inputType="number"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/Sberas"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <EditText
        android:id="@+id/bgula"
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:layout_marginStart="150dp"
        android:ems="10"
        android:hint="0"
        android:inputType="number"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/Sberas"
        app:layout_constraintTop_toBottomOf="@+id/bberas" />

    <EditText
        android:id="@+id/bminyak"
        android:layout_width="30dp"
        android:hint="0"
        android:layout_height="50dp"
        android:layout_marginStart="150dp"
        android:ems="10"
        android:inputType="number"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/Sminyak"
        app:layout_constraintTop_toBottomOf="@+id/bgula" />

    <EditText
        android:id="@+id/bgaram"
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:layout_marginStart="150dp"
        android:ems="10"
        android:hint="0"
        android:inputType="number"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/Sgaram"
        app:layout_constraintTop_toBottomOf="@+id/bminyak" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="120dp"
        android:text="Total Pembelian :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/telur" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="31dp"
        android:layout_marginTop="109dp"
        android:layout_marginEnd="31dp"
        android:text="Harga"
        app:layout_constraintEnd_toStartOf="@+id/textView17"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView16"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/hberas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="12dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/Sberas"
        app:layout_constraintTop_toBottomOf="@+id/textView24" />

    <TextView
        android:id="@+id/hgula"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/Sgula"
        app:layout_constraintTop_toBottomOf="@+id/hberas" />

    <TextView
        android:id="@+id/hminyak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/Sminyak"
        app:layout_constraintTop_toBottomOf="@+id/hgula" />

    <TextView
        android:id="@+id/hgaram"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/Stelur"
        app:layout_constraintTop_toBottomOf="@+id/hminyak" />

    <TextView
        android:id="@+id/htelur"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/Stelur"
        app:layout_constraintTop_toBottomOf="@+id/hgaram" />

    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Beli"
        android:onClick="checkout"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="119dp" />

    <TextView
        android:id="@+id/totalbeli"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="120dp"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/textView23"
        app:layout_constraintTop_toBottomOf="@+id/Stelur" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:onClick="belanja"
        android:text="Check out"
        app:layout_constraintBottom_toTopOf="@+id/totalbeli"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btelur"
        app:layout_constraintVertical_bias="0.49" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:text="Alamat penerima:"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toStartOf="@+id/alamatpenerima"
        app:layout_constraintTop_toBottomOf="@+id/textView23"
        app:layout_constraintVertical_bias="0.363" />

    <EditText
        android:id="@+id/alamatpenerima"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="94dp"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="94dp"
        android:ems="10"
        android:hint="Alamat penerima pesanan"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

</androidx.constraintlayout.widget.ConstraintLayout>